{
  "version": 2,
  "functions": {
    "api/health.js": {
      "maxDuration": 10
    },
    "api/circulars.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/public/cms/circulars",
      "dest": "/api/circulars.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/health.js"
    }
  ]
}
