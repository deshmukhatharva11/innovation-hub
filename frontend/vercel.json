{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "env": {
    "CI": "false",
    "DISABLE_ESLINT_PLUGIN": "true",
    "REACT_APP_API_URL": "https://web-production-33931.up.railway.app"
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|svg|ico|pdf|csv))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
